class Solution:
    def numJewelsInStones(self, J, S):
        """
        :type J: str
        :type S: str
        :rtype: int
        """
        s = set(J)
        res = 0
        #for j in J:
        #    s.add(j)
        for letter in S:
            if letter in s:
                res += 1
        return res
